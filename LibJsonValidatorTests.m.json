{
    "package": {
        "LibJsonValidatorTests": {
            "type": "executable",
            "machine": [
                "host"
            ],
            "host_tools": {
                "cxx": {
                    "flags": "-std=c++17 -Wall -Wextra -ggdb3 -O2 -fexceptions"
                }
            },
            "source": [
                "Libraries/LibJsonValidator/*.cpp",
